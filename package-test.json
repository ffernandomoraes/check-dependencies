  "private": true,
  "scripts": {
    "start": "env-cmd -f .env -f .env.dev craco start",
    "build": "env-cmd -f .env -f .env.production craco build",
    "build:dev": "env-cmd -f .env -f .env.dev craco build",
    "build:qa": "env-cmd -f .env -f .env.qa craco build",
    "build:qa-fidc": "env-cmd -f .env -f .env.qa-fidc craco build",
